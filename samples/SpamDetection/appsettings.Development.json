{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning",
        "Azure": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/spam-detection-.log",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "PdfImageExtraction": {
    "Enabled": true,
    "ExtractorKey": "pdfpig",
    "MaxPagesToProcess": 0,
    "MaxImageSizeBytes": 5242880,
    "PreferredFormat": "jpeg",
    "JpegQuality": 85,
    "EncodeAsBase64": true,
    "LogDetailedResults": true,
    "SkipTextOnlyPages": true,
    "MinImageWidth": 50,
    "MinImageHeight": 50
  }
}
